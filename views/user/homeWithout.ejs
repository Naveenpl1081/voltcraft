    align-items: center;
    justify-content: center;
    transition: background-color 0.3s ease, border-color 0.3s ease;
}

.action-button:hover {
    background-color: #DB4444;
    border-color: #DB4444;
    color: white;
}

.product-info {
    padding: 16px;
    text-align: center;
}

.product-title {
    font-size: 18px;
    font-weight: 600;
    margin-bottom: 8px;
    color: #333;
}

.price-container {
    display: flex;
    gap: 8px;
    justify-content: center;
    margin-bottom: 8px;
}

.current-price {
    font-size: 18px;
    font-weight: 700;
    color: #DB4444;
}

.original-price {
    font-size: 16px;
    color: #999;
    text-decoration: line-through;
}

.rating {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 4px;
}

.stars {
